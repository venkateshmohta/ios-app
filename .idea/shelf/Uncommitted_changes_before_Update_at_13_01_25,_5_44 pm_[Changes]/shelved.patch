<+>//\n//  W2NUtilities.swift\n//  WebToNative\n//\n//  Created by Akash Kamati on 24/06/24.\n//  Copyright  2024 WebToNative. All rights reserved.\n//\n\nimport Foundation\nimport UIKit\nimport WebToNativeCore\n\n/**\n Checks if two URLs are equal after normalizing them.\n\n - Parameters:\n   - url1: The first URL string to compare.\n   - url2: The second URL string to compare.\n - Returns: `true` if the normalized URLs are equal, otherwise `false`.\n */\nfunc areUrlsEqual(_ url1: String?, _ url2: String?) -> Bool {\n    if(url1 == nil || url2 == nil){\n        return false\n    }\n    guard let normalizedUrl1 = normalizeUrl(url1!),\n          let normalizedUrl2 = normalizeUrl(url2!) else {\n        return false\n    }\n\n    return normalizedUrl1 == normalizedUrl2\n}\n\n/**\n Normalizes a given URL string.\n\n - Parameter urlString: The URL string to normalize.\n - Returns: A normalized `URL` object if successful, otherwise `nil`.\n */\nfunc normalizeUrl(_ urlString: String) -> URL? {\n    guard var components = URLComponents(string: urlString) else {\n        return nil\n    }\n\n    // Normalize scheme (http or https)\n    if components.scheme == \"http\" || components.scheme == \"https\" {\n        components.scheme = components.scheme?.lowercased()\n    } else {\n        return nil // Invalid scheme\n    }\n\n    // Remove \"www.\" from the host if it exists\n    if let host = components.host, host.hasPrefix(\"www.\") {\n        components.host = String(host.dropFirst(4))\n    }\n    \n    // Remove \"m.\" from the host if it exists\n    if let host = components.host, host.hasPrefix(\"m.\"){\n        components.host = String(host.dropFirst(2))\n    }\n\n    // Remove trailing slashes from the path\n    if let lastChar = components.path.last, lastChar == \"/\" {\n        components.path = String(components.path.dropLast())\n    }\n    return components.url\n}\n\n/**\n A subclass of UIButton that customizes the layout to display an image above a title with specified spacing.\n Adjust the `spacing` property to change the vertical distance between the image and the title.\n */\nclass CustomButtonWithImage: UIButton {\n    let spacing: CGFloat = 8.0 // Adjust the spacing between image and title as per your requirement\n    \n    override init(frame: CGRect) {\n        super.init(frame: frame)\n        setupButton()\n    }\n    \n    required init?(coder aDecoder: NSCoder) {\n        super.init(coder: aDecoder)\n        setupButton()\n    }\n    \n    private func setupButton() {\n        // Set content vertical alignment to center\n        contentVerticalAlignment = .center\n    }\n    \n    override func layoutSubviews() {\n        super.layoutSubviews()\n        \n        if let imageView = imageView, let titleLabel = titleLabel {\n            // Center the image horizontally\n            let imageCenterX = bounds.width / 2\n            imageView.center.x = imageCenterX\n            \n            // Set the image above the title\n            imageView.frame.origin.y = (bounds.height - imageView.frame.height - titleLabel.frame.height - spacing) / 2\n            \n            // Center the title horizontally\n            let titleCenterX = bounds.width / 2\n            titleLabel.center.x = titleCenterX\n            titleLabel.numberOfLines = 0; // Dynamic number of lines\n        //    titleLabel.lineBreakMode = NSLineBreakMode.byTruncatingTail\n            \n            // Set the title below the image with spacing\n            titleLabel.frame.origin.y = imageView.frame.maxY + spacing\n        }\n    }\n}\n\n/// For open app setting in phone setting\nfunc openAppSettingsPage() {\n    if let url = URL(string: UIApplication.openSettingsURLString) {\n        if UIApplication.shared.canOpenURL(url) {\n            UIApplication.shared.open(url, options: [:], completionHandler: nil)\n        }\n    }\n}\n\n/// for enabling and disabling sleep mode\nfunc keepScreenOn(flag: Bool) {\n    if flag {\n        UIApplication.shared.isIdleTimerDisabled = true\n\n    } else {\n        UIApplication.shared.isIdleTimerDisabled = false\n\n    }\n    \n}\n\nfunc checkFirstCallWhenAppStarted(){\n    WebToNativeCore.sendDataToWebView(data: [\n        \"type\":\"firstCallWhenAppStarted\",\n        \"result\": WebToNativeCore.funFirstCallWhenAppStarted\n    ])\n    WebToNativeCore.funFirstCallWhenAppStarted = false\n}\n\n/// Checks if bottom notch is supported for device\nfunc isBottomNotchSupported() -> Bool{\n    return UIApplication.shared.windows[0].safeAreaInsets.bottom > 0.0\n}\n\n/// Check if status bar is supported\nfunc isStatusBarSupported() -> Bool{\n    return UIApplication.shared.windows[0].safeAreaInsets.top > 0.0\n}\n\n/// checks if device form factor is iPad\nfunc isIPad() -> Bool{\n    return UIDevice.current.userInterfaceIdiom == .pad\n}\n\n/// this returns all the device info back to website when called through interface function\nfunc getDeviceInfo(){\n    let bundleID = Bundle.main.bundleIdentifier;\n    let appVersion = Bundle.main.infoDictionary?[\"CFBundleShortVersionString\"] as? String\n    let appBuild = Bundle.main.infoDictionary?[\"CFBundleVersion\"] as? String\n    let locale = NSLocale.current.languageCode;\n    let systemVersion = UIDevice.current.systemVersion\n    var localTimeZoneIdentifier: String { return TimeZone.current.identifier }\n    let deviceId = UIDevice.current.identifierForVendor!.uuidString\n    let deviceName = UIDevice.current.name.replacingOccurrences(of: \"'\", with: \" \")\n    let responseObject: [String: Any] = [\n        \"appId\": bundleID!,\n        \"appVersion\": appVersion!,\n        \"appVersionCode\": appBuild!,\n        \"installationId\": deviceId,\n        \"language\": locale!,\n        \"model\": deviceName,\n        \"os\": \"iOS\",\n        \"osVersion\": systemVersion,\n        \"platform\": \"ios\",\n        \"timeZone\": localTimeZoneIdentifier,\n        \"reqType\":\"deviceInfo\"\n    ]\n    WebToNativeCore.sendDataToWebView(data: responseObject)\n}\n
