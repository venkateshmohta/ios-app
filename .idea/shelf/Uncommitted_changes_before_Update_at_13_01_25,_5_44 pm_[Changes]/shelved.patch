<+>//\n//  SplashScreen.swift\n//  WebToNative\n//\n//  Created by Akash Kamati on 22/05/24.\n//  Copyright  2024 WebToNative. All rights reserved.\n//\n\nimport SwiftUI\nimport Lottie\nimport WebToNativeCore\nimport WebToNativeBiometric\n\n/**\n Represents the splash screen displayed when the application launches, integrating animations and biometric authentication if configured.\n\n - Parameters:\n   - ignoredEdges: A set of edges to be ignored for safe area insets.\n   - showBottomNotchBar: A Boolean indicating whether the bottom notch bar should be displayed.\n   - appConfig: A binding to `WebToNativeConfigData` providing configuration data for the splash screen.\n   - biometricAuthCompleted: A binding indicating whether biometric authentication has been completed.\n   - isStatusBarHidden: A Boolean indicating whether the status bar should be hidden.\n   - onSplashEnded: A closure to be executed when the splash screen ends.\n\n The `SplashScreen` struct manages the display of a splash screen based on configuration parameters such as background color, animations, and optional splash images. It handles biometric authentication if enabled, triggering actions based on authentication success or failure.\n\n - Note: This view supports both animated splash screens using Lottie animations and static splash images based on configuration flags in `WebToNativeConfig`.\n\n - Important: Biometric authentication is handled asynchronously using `WebToNativeBiometric` if configured and displays appropriate messages or exits the application on failure.\n\n */\nstruct SplashScreen: View{\n    let ignoredEdges: Edge.Set\n    let showBottomNotchBar:Bool\n    @Binding var appConfig:WebToNativeConfigData\n    @Binding var biometricAuthCompleted:Bool\n    let isStatusBarHidden:Bool\n    let onSplashEnded:()->Void\n    @State var isSplashEnded = false\n    @State var statusBarColor:String = if WebToNativeConfig.sharedConfig?.splashWithAnimation == true {\n        WebToNativeConfig.sharedConfig?.animationBGColor ?? \"\"\n    } else if WebToNativeConfig.sharedConfig?.showSplashImage == true {\n        WebToNativeConfig.sharedConfig?.splashBackgroundColor ?? \"\"\n    }\n    else {WebToNativeConfig.sharedConfig?.statusBarColor ?? \"\"}\n    \n    @State var showFullSplashImage = false\n    \n    var body: some View {\n        ZStack{\n            Color(UIColor(hexString:(WebToNativeConfig.sharedConfig?.splashWithAnimation == true) ? appConfig.animationBGColor ?? \"#FFFFFF\" : appConfig.splashBackgroundColor ?? \"#FFFFFF\"))\n            VStack{\n                if(WebToNativeConfig.sharedConfig?.splashWithAnimation == true){\n                    let enableFullSplash = appConfig.splashFullScreen ?? false\n                    if enableFullSplash {\n                        LottieView(animation: .named(\"Lottie\"))\n                            .looping()\n                            .scaledToFill()\n                    }\n                    else {\n                        LottieView(animation: .named(\"Lottie\"))\n                            .looping()\n                    }\n                }else if(WebToNativeConfig.sharedConfig?.showSplashImage == true){\n                    if let image =  UIImage(named: \"splashimg.png\"){\n                        Image(uiImage:image).resizable()\n                            .aspectRatio(contentMode: .fill)\n                            .frame(width: CGFloat(appConfig.splashIconWidth ?? 200),height: CGFloat(appConfig.splashIconHeight ?? 400))\n                            .clipShape(RoundedRectangle(cornerRadius: 0))\n                    }\n                    \n                }else{\n                    if let image = UIImage(named: \"splashimg.png\"){\n                        Image(uiImage: image).resizable()\n                            .aspectRatio(contentMode: .fill)\n                            .clipShape(RoundedRectangle(cornerRadius: 0))\n                            .clipped()\n                            .ignoresSafeArea(.all)\n                        //   .padding()\n                    }\n                }\n            \n            }.onChange(of: biometricAuthCompleted){ completed in\n                if completed && isSplashEnded{\n                    onSplashEnded()\n                }\n            }.onChange(of: isSplashEnded){ended in\n                if biometricAuthCompleted && isSplashEnded{\n                    onSplashEnded()\n                }\n            }\n            .onAppear{\n                if(WebToNativeConfig.sharedConfig?.splashWithAnimation != true && WebToNativeConfig.sharedConfig?.showSplashImage != true){\n                    showFullSplashImage = true\n                }\n                let splashTiming = appConfig.splashTiming ?? 4\n                DispatchQueue.main.asyncAfter(deadline: .now() + .seconds(splashTiming)){\n                    isSplashEnded = true\n                }\n                handleBiometricAuth()\n            }.padding((showBottomNotchBar  && isBottomNotchSupported()) ? [.bottom] : [])\n            \n            StatusBarAndBottomNotch(showStatusBar: showFullSplashImage ? false : !isStatusBarHidden, showBottomNotchBar: showFullSplashImage ? false : showBottomNotchBar, statusBarColor:$statusBarColor, bottomNotchColor: statusBarColor)\n\n        }.navigationBarHidden(true).statusBar(hidden: isStatusBarHidden).edgesIgnoringSafeArea(ignoredEdges)\n        \n    }\n    \n    \n    \n    /**\n     Handles biometric authentication if enabled, initiating the authentication process and managing authentication completion or failure.\n\n     - Note: If biometric authentication is enabled and not already completed (`biometricAuthCompleted` is false), the method starts biometric authentication using `WebToNativeBiometric.shared`. Upon completion, it updates `biometricAuthCompleted` based on success or failure and optionally exits the application in case of failure.\n     */\n    private func handleBiometricAuth(){\n        if(WebToNativeBiometric.shared.showBiometricAuthOnLoad() == true && !biometricAuthCompleted){\n            WebToNativeBiometric.shared.startBiometricAuth(\n                title: nil) { flag, type, error in\n                    if(flag || error == .biometryNotAvailable || error == .biometryNotEnrolled || error == .unknown){\n                        biometricAuthCompleted = true\n                    }else{\n                        WebToNativeCore.sendDataToWebView(data: [\n                            \"isSuccess\": false,\n                            \"type\":\"showBiometric\",\n                            \"error\":error?.w2nKey as Any\n                        ])\n                        DispatchQueue.main.asyncAfter(deadline: .now() + 0.5) {\n                            exit(0)\n                        }\n                    }\n                }\n        }else{\n            print(\"Biometric auth not enabled\")\n            biometricAuthCompleted = true\n        }\n    }\n}\n
